{
  "name": "e_learning",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "@sap/hana-client": "^2.27.19",
    "express": "^4",
    "hdb": "^2.26.4",
    "@sap/xssec": "^4"
  },
  "engines": {
           "node": "22.x"
        },
  "scripts": {
    "start": "cds-serve"
  },
  "private": true,
  "cds": {
    "build": {
      "tasks": [
        {
          "for": "hana",
          "dest": "../db"
        },
        {
          "for": "node-cf"
        }
      ]
    },
    "requires": {
      "db": {
        "kind": "hana-cloud"
      }
    }
  }
}
